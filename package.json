{
  "name": "webpack-3-practice",
  "version": "1.0.0",
  "description": "webpack 3.x practice",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist ci",
    "build": "npm run clean && webpack --config webpack/config.js --progress",
    "watch": "webpack --progress --watch --config webpack/dev.config.js",
    "lint": "eslint webpack entry example component --fix",
    "test": "mocha-webpack test/**/*.js --webpack-config test/config.js --opts test/mocha.opts",
    "coverage": "cross-env NODE_ENV=test nyc mocha-webpack test/**/*.js --webpack-config test/config.js --opts test/mocha.opts --reporter mocha-junit-reporter --reporter-options mochaFile=./ci/test-results.xml",
    "start": "babel-node webpack/dev.server",
    "start-express": "babel-node server"
  },
  "author": "intoday_@naver.com",
  "license": "ISC",
